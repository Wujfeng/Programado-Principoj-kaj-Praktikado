// Description: An elementary calculator.
// Author: Четан Ананд (verdastelo)

#include "std_lib_facilities.h"
using namespace std;

int main() {
  class Token {
  public:
      char kind;
      double value;
      Token (char ch)
              :kind (ch), value (0) {}
      Token (char ch, double val)
              :kind (ch), value (val) {}
  };
  return 0;
}