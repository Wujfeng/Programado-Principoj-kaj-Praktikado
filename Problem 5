#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int main()

{
    char z;
    double x, y = 0.0;

    cout << "Enter two numbers (separated by SPACE) followed by +, -, *, /, or %: \n";
    cin >> x >> y;
    cin >> z;
    if (z == '+')
        cout << x + y << '\n';
    if (z == '-')
        cout << x - y << '\n';
    if (z == '*')
        cout << x * y << '\n';
    if (z == '/')
        cout << x / y << '\n';
    if (z == '%') {
        int i = 0;
        cout << (i = (x/y)) << '\n';
        double j = 0.0;
        cout << (j = ((x/y) - i)) << '\n';
        cout << y * j << '\n';
        }

    return 0;
}
