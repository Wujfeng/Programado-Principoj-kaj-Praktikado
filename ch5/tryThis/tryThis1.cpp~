#include <iostream> 

int area (int length, int width)
{
    return length * width; 
}

int main()
{
    // int s1 = area (7; // error expected ')' before ';' token
    // int s1 = area (7) // erro: too few arguments 
    // Int s3 = area (7); // Int was not declared in this scope 
    int s4 = area ('7); // missing terminating character 
    return 0;
}
